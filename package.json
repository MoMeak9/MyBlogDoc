{
  "name": "my-blog-doc",
  "version": "2.0.0",
  "description": "A project of vuepress-theme-hope",
  "license": "MIT",
  "scripts": {
    "docs:build": "vuepress build src",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "docs:dev": "vuepress dev src",
    "docs:deploy": "serverless deploy",
    "git:commit": "git fetch && git add -A && git-cz && git pull && git push",
    "doc:commitAndDeploy": "npm run git:commit && npm run docs:deploy"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@vuepress/client": "2.0.0-beta.53",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "vue": "^3.2.45",
    "vuepress": "^2.0.0-beta.53",
    "vuepress-plugin-search-pro": "2.0.0-beta.124",
    "vuepress-theme-hope": "^2.0.0-beta.121"
  }
}
