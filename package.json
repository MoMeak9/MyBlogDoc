{
  "name": "my-blog-doc",
  "version": "2.0.0",
  "description": "A project of vuepress-theme-hope",
  "license": "MIT",
  "scripts": {
    "docs:build": "vuepress build src",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "docs:dev": "vuepress dev src",
    "deploy": "npm run docs:build && components deploy",
    "commit": "git add -A && git-cz && git push"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@vuepress/client": "^2.0.0-beta.43",
    "@vuepress/plugin-search": "2.0.0-beta.48",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "vue": "^3.2.33",
    "vuepress": "^2.0.0-beta.43",
    "vuepress-theme-hope": "^2.0.0-beta.54"
  }
}
